<Window x:Class="exercice_5.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:exercice_5"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="800">
    <Window.Resources>
        <BitmapImage x:Key="computeur" UriSource="Exercice5_computer.png" />
        <BitmapImage x:Key="lamanu" UriSource="Exercice5_laManu.jpg" />
    </Window.Resources>
    <Grid>
        <Image Source="{StaticResource ResourceKey=computeur}" />
        <Image Source="{StaticResource ResourceKey=lamanu}" Margin="28,48,56,189" />
    </Grid>
</Window>
